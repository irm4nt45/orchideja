  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:functions": "npm run --prefix functions build",
    "postbuild": "babel node_modules/@agm/core -d functions/@agm/core --presets @babel/preset-env && node ./copy-package-json.js",
    "postbuild2": "tsc -p tsconfig.agm.json --allowJs && node ./copy-package-json.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "firebase serve",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server && npm run build:functions",
    "build:client-and-server-bundles": "ng build --prod --optimization=false && ng run orchideja:server:production --bundleDependencies none"
  },
  "private": true,
